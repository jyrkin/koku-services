<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd"
         version="3.0">
    <enterprise-beans>

        <session>
            <ejb-name>KksCollectionsDAOImpl</ejb-name>
            <env-entry>
                <env-entry-name>uid</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>changeit</env-entry-value>
                <injection-target>
                    <injection-target-class>fi.arcusys.koku.tiva.service.impl.KksCollectionsDAOImpl</injection-target-class>
                    <injection-target-name>uid</injection-target-name>
                </injection-target>
            </env-entry>
            <env-entry>
                <env-entry-name>pwd</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>changeit</env-entry-value>
                <injection-target>
                    <injection-target-class>fi.arcusys.koku.tiva.service.impl.KksCollectionsDAOImpl</injection-target-class>
                    <injection-target-name>pwd</injection-target-name>
                </injection-target>
            </env-entry>
            <env-entry>
                <env-entry-name>component</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>tiva</env-entry-value>
                <injection-target>
                    <injection-target-class>fi.arcusys.koku.tiva.service.impl.KksCollectionsDAOImpl</injection-target-class>
                    <injection-target-name>component</injection-target-name>
                </injection-target>
            </env-entry>
            <env-entry>
                <env-entry-name>urlBase</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>http://localhost:8080</env-entry-value>
                <injection-target>
                    <injection-target-class>fi.arcusys.koku.tiva.service.impl.KksCollectionsDAOImpl</injection-target-class>
                    <injection-target-name>urlBase</injection-target-name>
                </injection-target>
            </env-entry>
        </session>
    </enterprise-beans>
</ejb-jar>
